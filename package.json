{
  "name": "vite-plugin-remix-flat-routes",
  "version": "4.0.0",
  "description": "remix-flat-routes convention-based routing, compatible with react-router-dom>=6 data routing and non-data routing",
  "type": "module",
  "keywords": [
    "vite",
    "remix",
    "react-router",
    "remix-router",
    "remix-routes",
    "plugin"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemengke1997/vite-plugin-remix-flat-routes.git"
  },
  "author": "hemengke <https://github.com/hemengke1997>",
  "files": [
    "*.d.ts",
    "dist/"
  ],
  "main": "./dist/node/index.cjs",
  "module": "./dist/node/index.js",
  "types": "./dist/node/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/node/index.d.ts",
      "require": "./dist/node/index.cjs",
      "import": "./dist/node/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "require": "./dist/client/index.cjs",
      "import": "./dist/client/index.js"
    },
    "./virtual": {
      "types": "./virtual.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "client": [
        "dist/client"
      ]
    }
  },
  "sideEffects": false,
  "scripts": {
    "dev": "tsup --watch",
    "build": "rm -rf dist && tsup",
    "test": "vitest --run",
    "commitlint": "commitlint -e",
    "prepare": "simple-git-hooks",
    "lint": "tsc --noEmit && eslint .",
    "up": "taze -I -w -r",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "bump": "bumpp package.json -c --no-push -t --all -x \"npm run changelog\""
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17",
    "react-router": ">=6",
    "react-router-dom": ">=6",
    "vite": ">=5"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "react-router-dom": {
      "optional": true
    },
    "react-router": {
      "optional": true
    }
  },
  "dependencies": {
    "change-case": "^5.4.4",
    "context-state": "^3.1.2",
    "es-module-lexer": "^1.5.4",
    "local-pkg": "^0.5.0",
    "minimatch": "5.1.0",
    "semver": "^7.6.3",
    "serialize-javascript": "^6.0.2",
    "type-fest": "^4.26.1",
    "vite-node": "^2.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@minko-fe/commitlint-config": "^2.1.2",
    "@minko-fe/eslint-config": "^4.1.0",
    "@minko-fe/prettier-config": "^2.2.3",
    "@minko-fe/tsconfig": "^2.1.1",
    "@remix-run/dev": "^2.13.1",
    "@remix-run/react": "^2.13.1",
    "@types/clone-deep": "^4.0.4",
    "@types/minimatch": "^5.1.2",
    "@types/node": "^22.7.6",
    "@types/react": "^18.3.11",
    "@types/semver": "^7.5.8",
    "@types/serialize-javascript": "^5.0.4",
    "bumpp": "^9.7.1",
    "clone-deep": "^4.0.1",
    "conventional-changelog-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.12.0",
    "npm-run-all": "^4.1.5",
    "p-is-promise": "^4.0.0",
    "postcss": "^8.4.47",
    "postcss-config-preset": "^1.0.0",
    "react": "^18.3.1",
    "remix-flat-routes": "^0.6.5",
    "simple-git-hooks": "^2.11.1",
    "tailwindcss": "^3.4.14",
    "taze": "^0.17.2",
    "tsup": "^8.3.0",
    "tsup-plugin-bundleless": "^1.1.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.9",
    "vitest": "^2.1.3"
  },
  "simple-git-hooks": {
    "commit-msg": "npm run commitlint",
    "pre-commit": "npm run lint"
  },
  "commitlint": {
    "extends": [
      "@minko-fe/commitlint-config"
    ]
  },
  "prettier": "@minko-fe/prettier-config"
}
